{
  "name": "@traverse-data/sails-dynamodb",
  "version": "1.0.0",
  "description": "Amazon DynamoDB adapter for Sails / Waterline",
  "main": "index.js",
  "scripts": {
    "test": "node test/integration/runner -R spec -b",
    "release": "git commit -am $npm_package_version && git tag $npm_package_version && git push && git push --tags && npm publish --access public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/TraverseData/sails-dynamodb.git"
  },
  "keywords": [
    "dynamodb",
    "aws",
    "adapter",
    "sails",
    "waterline",
    "orm",
    "sailsjs",
    "sails.js"
  ],
  "author": "gadelkareem",
  "license": "MIT",
  "dependencies": {
    "@traverse-data/vogels": "0.12.2",
    "lodash": "^3.10.1",
    "async": "^1.5.0"
  },
  "devDependencies": {
    "mocha": "~1.13.0",
    "waterline-adapter-tests": "~0.10.0",
    "captains-log": "~0.11.1"
  },
  "waterlineAdapter": {
    "waterlineVersion": "~0.10.x",
    "interfaces": [
      "semantic",
      "queryable",
      "associations"
    ]
  },
  "bugs": {
    "url": "https://github.com/TraverseData/sails-dynamodb/issues"
  },
  "homepage": "https://github.com/TraverseData/sails-dynamodb",
  "directories": {
    "test": "test"
  }
